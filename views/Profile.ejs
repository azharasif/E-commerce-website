                                <div>
                                    <a href="/Register">Register</a>
                                </div>
                                <div>
                                    <a href="/Login">Sign in</a>
                                </div>

                                <% } else { %>
                                    <div>
                                        <a href="/profile">
                                            <%=  person.Name %>
                                        </a>
                                    </div>
                        </div>
                        <a href="/logout">Logout</a>
                    </div>

                    <% } %>

                </div>


            </div>
        </div>
