                                <div class="top_bar_user">
                                    <div class="user_icon">
                                        <img src="images/user.svg" alt="">
                                    </div>
                                    <% if(!person) { %>
                                        <div>
                                            <a href="/Register">Register</a>
                                        </div>
                                        <div>
                                            <a href="/Login">Sign in</a>
                                        </div>
                                        <% } else { %>
                                            <div>
                                                <a href="/profile">
